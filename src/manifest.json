{
  "manifest_version": 3,
  "name": "Webel",
  "description": "Web browsing event logger",
  "version": "1.0",
  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "48": "images/icon-48.png",
    "128": "images/icon-128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "storage",
    "tabs",
    "webNavigation",
    "history",
    "notifications",
    "identity"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "oauth2": {
    "client_id": "697576008157-u5ov2fhfft40hhbilbjktui41piu7fka.apps.googleusercontent.com",
    "scopes": [
      "https://www.googleapis.com/auth/drive"
    ]
  },
  "key": ""
} 